<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zh.myfilter.dao.UploadDao">

    <insert id="insertImage">
         insert into stuimg (uid, url, date, name)
         values (#{uid}, #{url},now(), #{name})
    </insert>



</mapper>
